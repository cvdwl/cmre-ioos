    <fmrcConfig regularize="False" datasetTypes="Best"/>

 <netcdf xmlns="http://www.unidata.ucar.edu/namespaces/netcdf/ncml-2.2">
     <variable name="time">
      <attribute name="units" value="days since -0001-12-31 00:00"/>
